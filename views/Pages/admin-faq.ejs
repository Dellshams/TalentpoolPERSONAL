  <link rel="shortcut icon" href="/img/favicon.svg" type="image/x-icon">

</head>

<body>

  <!-- Top Navbar -->
  <%- include('../Templates/admin-header') %>

  <!-- Side Nav -->
  <%- include('../Templates/admin-sidebar') %>

  <div id="body__click"></div>

  <!-- FAQ Page -->
  <div id="heading">
    FAQs
  </div>

  <div class="faq_section container">
    <div class="row no-gutters">
      <div class="faq_sidenav col-sm-3">
        <ul id="link-container" class="faq_sidenav_options">
          <li><a class="li active" href="#talent">TalentPool</a></li>
          <li><a class="li" href="#getting-started">Getting Started</a></li>
          <li><a class="li" href="#application" class="">Application</a></li>
          <li><a class="li" href="#membership">Membership</a></li>
          <li>
            <div class="faq_button_div"><button class="btn btn-link faq_button" type="button" data-toggle="modal"
                data-target="#exampleModal"> <img src="/img/plus-20.png" style="margin-right: 8px; scale: .8;"> Add FAQ</button></div>

          </li>
        </ul>

      </div>
      <div id="faq_pageTP" class="col-sm-9 talent">
        <div class="row">
          <div class="pricing">Talent</div>
          <div class="edit editTP"><a href="#">Edit</a></div>
        </div>

        <div class="faq_items">
          <div class="accordion" id="questions">
            <% for(let i = 0;i<data.length;++i) { %>
              <% if(data[i].category=="TP") { %>
            <div class="row container align-items-center">
              <div class="card col-sm-10">

                <div type="button" data-toggle="collapse" data-target="#collapse<%= i %>" aria-expanded="true"
                  aria-controls="collapse<%= i %>" class="card-header" id="heading<%= i %>">
                  <div class="d-flex justify-content-between    align-items-center">
                    <div class="d-flex align-items-center"><i class="fa fa-plus" aria-hidden="true"></i> <%= data[i].question %>
                    </div>

                  </div>

                </div>



                <div id="collapse<%= i %>" class="collapse" aria-labelledby="heading<%= i %>" data-parent="#questions">
                  <div class="card-body">
                    <%= data[i].answer %>
                  </div>
                </div>
              </div>
              <div class="col-sm-2">

                <div class="d-none justify-content-between admin-action admin-actionTP  ">
                  <!-- <button class="btn-primary  btn mx-2 align-middle"><i class="fas fa-edit" aria-hidden="true"></i>
                  </button> -->
                <button class="btn-primary  btn mx-2 align-middle edit-btn" id="edit<%= i %>" value="<%= data[i].id %>%%<%=data[i].answer%>%%<%=data[i].question %>%%<%= data[i].category %>"> <i class="fas fa-edit" aria-hidden="true"></i></button>


                    <input id = 'idTP<%= i %>' type="text" name="id" value="<%= data[i].id %>" hidden = true>

                      <button id='deleteTP <%= i %>'  class="btn-danger btn delete-btn">
                        <i class="fa fa-trash" aria-hidden="true">
                        </i>

                      </button>


                </div>

                <div class="custom-control custom-switch togglerTP">
                  <% if(data[i].blocked==0) {%>
                    <input type="checkbox" class="custom-control-input customSwitch" id="customSwitchTP <%= i %>">
                    <% } %>
                  <% if(data[i].blocked==1) { %>
                  <input checked type="checkbox" class="custom-control-input customSwitch" id="customSwitchTP <%= i %>">
                  <% } %>
                  <label class="custom-control-label" for="customSwitchTP <%= i %>"></label>
                </div>

              </div>

            </div>
            <% } %>
            <% } %>


            <!--col-->
          </div> <!-- row -->
        </div>

      </div>

