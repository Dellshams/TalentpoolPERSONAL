                      <label class="input-group-text" for="inputGroupSelect01">Category</label>
                    </div>
                    <select class="custom-select" id="categoryUpdate" name="category">
                      <option value="TP" selected>Talent Pool</option>
                      <option value="GS">Getting Started</option>
                      <option value="AP">Application</option>
                      <option value="MB">Membership</option>
                    </select>
                  </div>
                </div>
                <div class="form-group">
                  <label for="recipient-name" class="col-form-label">Question:</label>
                  <input type="text" class="form-control" id="questionUpdate" name="question">
                </div>
                <input type="text" name="" id="idUpdate" value="" hidden= true>
                <div class="form-group">
                  <label for="message-text" class="col-form-label">Answer:</label>
                  <textarea class="form-control" id="answerUpdate" name="answer"></textarea>
                </div>
                <div class="modal-footer">
                  <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                  <button type="button" id="update-button" class="btn btn-primary">Update</button>
                </div>
              </form>
            </div>

          </div>
