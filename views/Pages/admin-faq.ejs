      <!-- Modal to Add New FAQs -->
      <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
        aria-hidden="true">
        <div class="modal-dialog" role="document">
          <div class="modal-content">
            <div class="modal-header">
              <h5 class="modal-title" id="exampleModalLabel">New FAQs</h5>
              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
            </div>
            <div class="modal-body">
              <form method="post">
                <input type="hidden" name="_csrf" value="<%= csrfToken %>">
                <div class="form-group">
                  <div class="input-group mb-3">
                    <div class="input-group-prepend">
                      <label class="input-group-text" for="inputGroupSelect01">Category</label>
                    </div>
                    <select class="custom-select" id="createCategory" name="category">
                      <option value="TP" >Talent Pool</option>
                      <option value="GS">Getting Started</option>
                      <option value="AP">Application</option>
                      <option value="MB">Membership</option>
                    </select>
                  </div>
                </div>
                <div class="form-group">
                  <label for="recipient-name" class="col-form-label">Question:</label>
                  <input placeholder="Enter the question" type="text" class="form-control" id="createQuestion" name="question">
                </div>
                <div class="form-group">
                  <label for="message-text" class="col-form-label">Answer:</label>
                  <textarea placeholder="Enter the answer" class="form-control" id="createAnswer" name="answer"></textarea>
                </div>
                <div class="modal-footer">
                  <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                  <button id="create_button" type="button" class="btn btn-primary">Submit</button>
                </div>
              </form>
            </div>

          </div>
        </div>
      </div>





